# Default Columns
#
plntname                        Beta Pic b  |\ # planet name
ra_str                            |\ # RA [sexagesimal]
dec_str                           |\ # Dec [sexagesimal]
#
# Imaging Columns
#
imdeltmag                       | imdeltmagerr		| imdeltmaglim        |\			# "imdeltmag", planet-host magnitude difference [mag]
imflxratio                      | imflxratioerr1        | imflxratioerr2       | imflxratiolim     |\ 	# "imflxratio", planet/host flux ratio
imangsep                        | imangseperr1          | imangseperr2         | imangseplim       |\  # "imangsep", star-planet angular separation [arcsec]
imsep                           | imseperr1             | imseperr2            | imseplim          |\  # "imsep", projected star-planet separation [AU]
imtruesep                       | imtrueseperr1         | imtrueseperr2        | imtrueseplim      |\  # "imtruesep", true star-planet separation [AU] 
impa                            | impaerr1              | impaerr2             | impalim           |\  # "impa", star-planet position angle [deg] 
imdist                          | imdisterr1            | imdisterr2           | imdistlim         |\  # "imdist", distance to star [pc]
imtelescope	               |\ # "imtelescope", Telescope used in observations ##added this
iminstrument.              |\ # "iminstrument", Instrument used in observations ##added this
imdateobs	                 |\ # "imdateobs", [YYYY-MM-DD] ##added this
imcluster	                 |\ # "imcluster", name of associated cluster
imdisk 		                 |\ # "imdisk", circumstellar disk detected (1=yes, 0=no)
imcorona		       |\ # "imcorona", Coronagraph used (1=yes, 0=no)
imifu 			       |\ # "imifu", IFU used (1=yes, 0=no)
#
# Astrometry Columns
#
ns			       |\ # "ns", number of stars constituting the host system
ra                          TBD |\ # RA [decimal]
dec                         TBD |\ # Dec [decimal]
glon                        TBD |\ # "l" [decimal]
glat                  	    TBD |\ # "b" [decimal]
#
# Photometry Columns
#
imjmag                     | imjmagerr1        | imjmagerr2       | imjmaglim         |\ # "imjmag", stellar magnitude J-band [mag]
imhmag                     | imhmagerr1        | imhmagerr2       | imhmaglim         |\ # "imhmag", stellar magnitude H-band [mag]
imkmag                     | imkmagerr1        | imkmagerr2       | imkmaglim         |\ # "imkmag", stellar magnitude K-band [mag]
imlpmag                    | imlpmagerr1       | imlpmagerr2      | imlpmaglim        |\ # "imlpmag", stellar magnitude Lp-band [mag]
implnjmag                  | implnjmagerr1     | implnjmagerr2    | implnjmaglim      |\ # "implnjmag", planet magnitude J-band [mag] 
implnhmag                  | implnhmagerr1     | implnhmagerr2    | implnhmaglim      |\ # "implnhmag", planet magnitude H-band [mag]
implnkmag                  | implnkmagerr1     | implnkmagerr2    | implnkmaglim      |\ # "implnkmag", planet magnitude K-band [mag]
implnlpmag                 | implnlpmagerr1    | implnlpmagerr2   | implnlpmaglim     |\ # "implnlpmag", planet magnitude Lp-band [mag]
#
# Stellar Parameter Columns
#
imstype			     |\ # "imstype", stellar host spectral type
immass                      | immasserr1           | immasserr2         | immasslim         |\ # "immass", stellar host [solar mass]
imlum                       | imlumerr1            | imlumerr2          | imlumlim          |\ # "imlum", stellar host [solar luminosity]
image		              | imageerr1	       | imageerr2	      | imagelim	   |\ # "image", stellar age [My]
#
# Model Columns
#
implstype			|\ # "implstype", planet spectral type
implmass                     | implmasserr1          | implmasserr2         | implmasslim       |\ # "implmass", planet mass [Jupiter mass]
impltemp                     | impltemperr1          | impltemperr2         | impltemplim       |\ # "impltemp", planet temp [K]
implradius                   | implradiuserr1        | implradiuserr2       | implradiuslim     |\ # "implradius", planet radius [Jupiter radii]
immodel		     Cloud-free	|\ # "immodel", description of model ##Reference that the model is drawn from.
immodeldef		      0 |\ # "immodeldef" (1=yes, 0=no)
